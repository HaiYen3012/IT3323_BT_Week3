PROGRAM TestAssign1
VAR n : INTEGER;
VAR i : INTEGER;
VAR j : INTEGER;
VAR matrix : ARRAY [100] OF ARRAY [100] OF INTEGER;
VAR isUpperTriangular : INTEGER;

PROCEDURE InputMatrix;
VAR row : INTEGER;
VAR col : INTEGER;
BEGIN
  FOR row := 0 TO n - 1 DO
  BEGIN
    FOR col := 0 TO n - 1 DO
    BEGIN
      matrix[row][col] := READI()
    END
  END
END;

FUNCTION CheckUpperTriangular : INTEGER;
VAR row : INTEGER;
VAR col : INTEGER;
VAR result : INTEGER;
BEGIN
  result := 1;
  FOR row := 1 TO n - 1 DO
  BEGIN
    FOR col := 0 TO row - 1 DO
    BEGIN
      IF matrix[row][col] != 0 THEN
        result := 0
    END
  END;
  CheckUpperTriangular := result
END;

BEGIN
  WRITEI(n);
  CALL InputMatrix;
  isUpperTriangular := CheckUpperTriangular();
  WRITEI(isUpperTriangular)
END.
